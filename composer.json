        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\IBAN\\": "src"
        }
    },
    "require-dev": {
        "consolidation/robo": "^4.0"
